# Database
DATABASE_URL=postgresql://user:password@localhost:5432/burnout_monitor

# JWT Secret (generate a random string)
JWT_SECRET=your-jwt-secret-here

# Environment
NODE_ENV=development

# Port (Heroku will set this automatically)
PORT=3001

# Twilio (optional - for SMS notifications)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=

# Google OAuth (optional - for calendar integration)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=

# Terra Health API (optional - for wearable device integration)
TERRA_API_KEY=
TERRA_DEV_ID=
TERRA_WEBHOOK_SECRET=
